{
  "name": "dice-server-js",
  "version": "0.1.0",
  "description": "Server for parsing and executing dice notation.",
  "main": "index.js",
  "dependencies": {
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "selenium-webdriver": "^2.46.1"
  },
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ssoloff/dice-server-js.git"
  },
  "author": "Steven Soloff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssoloff/dice-server-js/issues"
  },
  "homepage": "https://github.com/ssoloff/dice-server-js",
  "jscsConfig": {
    "excludeFiles": [
      "lib/dice-expression-parser.js",
      "node_modules/**",
      "out/**"
    ],
    "preset": "crockford",
    "requireMultipleVarDecl": null
  },
  "jshintConfig": {
    "jasmine": true,
    "node": true,
    "curly": true,
    "eqeqeq": true,
    "laxbreak": true,
    "undef": true,
    "unused": true
  }
}
