{
  "opts": {
    "destination": "build/docs/client",
    "readme": "README.md",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "public"
    ],
    "includePattern": ".+\\.js$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
