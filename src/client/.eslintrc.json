{
  "root": true,
  "extends": [
    "eslint:recommended",
    "crockford"
  ],
  "env": {
    "commonjs": true
  },
  "rules": {
    "guard-for-in": 2,
    "max-len": [2, {"code": 120}],
    "no-bitwise": 2,
    "no-caller": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-new": 2,
    "no-use-before-define": [2, "nofunc"],
    "strict": 2
  }
}
