{
  "opts": {
    "destination": ".build/docs/server",
    "readme": "README.md",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [
      "src/server",
      "test/server"
    ],
    "includePattern": ".+\\.js(doc)?$"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
