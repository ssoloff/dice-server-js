{
    "opts": {
        "recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "excludePattern": "(^|\\/|\\\\)_",
        "include": [
            "lib",
            "spec"
        ],
        "includePattern": ".+\\.js(doc)?$"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

