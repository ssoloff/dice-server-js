{
    "opts": {
        "recurse": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "excludePattern": "(^|\\/|\\\\)_",
        "include": ["."],
        "includePattern": ".+\\.js(doc)?$"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

